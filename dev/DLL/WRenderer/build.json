{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../"],
  "sources_path": ["Sources/*.cxx"],
  "output_name": "wrenderer.dll",
  "compiler_flags": [
    "-ffreestanding",
    "-shared",
    "-fno-rtti",
    "-fno-exceptions",
    "-Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__WRENDER_IMPL__",
    "cWRVersion=0x0100",
    "cWRVersionHighest=0x0100",
    "cWRVersionLowest=0x0100"
  ]
}
