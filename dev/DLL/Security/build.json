{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../"],
  "sources_path": ["Sources/*.cxx"],
  "output_name": "sec.dll",
  "compiler_flags": [
    "-ffreestanding",
    "-shared",
    "-fno-rtti",
    "-fno-exceptions",
    "-Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__SEC_IMPL__",
    "cSECVersion=0x0100",
    "cSECVersionHighest=0x0100",
    "cSECVersionLowest=0x0100"
  ]
}
