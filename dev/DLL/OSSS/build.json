{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../", "./"],
  "sources_path": ["*.cxx"],
  "output_name": "osss.dll",
  "compiler_flags": [
    "-ffreestanding",
    "-shared",
    "-fno-rtti",
    "-fno-exceptions",
    " -Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__OSSS_AMD64__",
    "cOSSSVersion=0x0100",
    "cOSSSVersionHighest=0x0100",
    "cOSSSVersionLowest=0x0100"
  ]
}
