{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../", "./", "../../"],
  "sources_path": ["Sources/*.cxx"],
  "output_name": "runxpcom.exe",
  "compiler_flags": [
    "-ffreestanding",
    "-fno-rtti",
    "-fno-exceptions",
    " -Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__SCM_AMD64__",
    "cSCMVersion=0x0100",
    "cSCMVersionHighest=0x0100",
    "cSCMVersionLowest=0x0100"
  ]
}
