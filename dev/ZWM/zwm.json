{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../"],
  "sources_path": ["Sources/*.cxx"],
  "output_name": "zwm.sys",
  "compiler_flags": [
    "-fPIC",
    "-ffreestanding",
    "-shared",
    "-fno-rtti",
    "-fno-exceptions",
    "-Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__ZWM_IMPL__",
    "__NEWOSKRNL__",
    "cZWMVersion=0x0100",
    "cZWMVersionHighest=0x0100",
    "cZWMVersionLowest=0x0100"
  ]
}
