{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../"],
  "sources_path": ["src/*.cc"],
  "output_name": "sci.dll",
  "compiler_flags": [
    "-fPIC",
    "-ffreestanding",
    "-shared",
    "-fno-rtti",
    "-fno-exceptions",
    "-Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__SCI_IMPL__",
    "cSCIVersion=0x0100",
    "cSCIVersionHighest=0x0100",
    "cSCIVersionLowest=0x0100"
  ]
}
