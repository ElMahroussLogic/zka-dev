{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../", "./"],
  "sources_path": ["src/*.cxx"],
  "output_name": "crt.dll",
  "compiler_flags": [
    "-ffreestanding",
    "-shared",
    "-fno-rtti",
    "-fno-exceptions",
    "-Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__CRT_AMD64__",
    "cCRTVersion=0x0100",
    "cEFSVersionHighest=0x0100",
    "cEFSVersionLowest=0x0100"
  ]
}
