{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../", "./"],
  "sources_path": ["src/*.cc"],
  "output_name": "efs.zsc",
  "compiler_flags": [
    "-ffreestanding",
    "-shared",
    "-fno-rtti",
    "-fno-exceptions",
    " -Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__EFS_AMD64__",
    "cEFSVersion=0x0100",
    "cEFSVersionHighest=0x0100",
    "cEFSVersionLowest=0x0100"
  ]
}
