{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../", "../../", "../../zka"],
  "sources_path": ["src/*.cc"],
  "output_name": "ViewPDF.exe",
  "compiler_flags": [
    "-fPIC",
    "-ffreestanding",
    "-shared",
    "-fno-rtti",
    "-fno-exceptions",
    "-Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__VIEW_PDF_IMPL__",
    "cPDFVersion=0x0100",
    "cPDFVersionHighest=0x0100",
    "cPDFVersionLowest=0x0100"
  ]
}
