{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../", "./", "../../"],
  "sources_path": ["src/*.cxx"],
  "output_name": "rundll64.exe",
  "compiler_flags": [
    "-ffreestanding",
    "-fno-rtti",
    "-fno-exceptions",
    "-Wl,--subsystem=17"
  ],
  "cpp_macros": [
    "__RUNDLL_AMD64__",
    "cRunDLLVersion=0x0100",
    "cRunDLLVersionHighest=0x0100",
    "cRunDLLVersionLowest=0x0100"
  ]
}
