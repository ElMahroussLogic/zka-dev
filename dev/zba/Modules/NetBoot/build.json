{
  "compiler_path": "x86_64-w64-mingw32-g++",
  "compiler_std": "c++20",
  "headers_path": ["../", "../../", "../../../zka", "../../../", "./"],
  "sources_path": [".cc", "*.S"],
  "output_name": "netboot.sys",
  "compiler_flags": [
    "-ffreestanding",
    "-nostdlib",
    "-std=c++20",
    "-fPIC",
    "-fno-rtti",
    "-fno-exceptions",
    "-Wl,--subsystem=17,--image-base,0x1000000"
  ],
  "cpp_macros": [
    "__NEWOSKRNL__",
    "__NEWOSLDR__",
    "__ZKA_AMD64__",
    "__BOOTSCR__",
    "cSCRVersionHighest=0x0100",
    "cSCRVersionLowest=0x0100",
    "cSCRVersion=0x0100"
  ]
}
